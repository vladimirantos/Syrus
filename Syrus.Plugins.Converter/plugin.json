{
  "name": "Converter",
  "fullName": "Syrus.Plugins.Converter",
  "author": "Vladimír Antoš",
  "version": "0.0.0.1",
  "icon": "icon.png",
  "enableHelp": false,
  "resultConfiguration": {
    "viewMode": "hidden"
  },
  "searchingConfigurations": [
    {
      "language": "en",
      "regularExpressions": [
        {
          "id": 1,
          "text": [ "^.*$" ]
        },
        {
          "id": 2,
          "text": [ "^[a-zA-Z]:\\\\[\\\\\\S|*\\S]?.*$" ]
        }
      ]
    },

    {
      "language": "cs",
      "regularExpressions": [
        {
          "id": 1,
          "text": [ "^.*$" ]
        },
        {
          "id": 2,
          "text": [ "^[a-zA-Z]:\\\\[\\\\\\S|*\\S]?.*$" ]
        }
      ]
    }
  ]
}

{
  "name": "Calculator",
  "fullName": "Syrus.Plugins.Calculator",
  "author": "Vladimír Antoš",
  "version": "0.0.0.1",
  "icon": "icon.png",
  "enableHelp": false,
  "searchingConfigurations": [
    {
      "language": "en",
      "regularExpressions": [
        {
          "id": 1,
          "text": [ "^\\s*([-+]?)(\\d+)(?:\\s*([-+*\\/])?\\s*((?:\\s[-+])?\\d+)?\\s*)+$" ]
        }
      ]
    },

    {
      "language": "cs",
      "regularExpressions": [
        {
          "id": 1,
          "text": [ "^\\s*([-+]?)(\\d+)(?:\\s*([-+*\\/])?\\s*((?:\\s[-+])?\\d+)?\\s*)+$" ]
        }
      ]
    }
  ]
}

<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm1="clr-namespace:Syrus.Plugins.Applications">
    <DataTemplate DataType="{x:Type vm1:AppInfo}">
        <StackPanel>
            <Label Content="{Binding Name}"/>
            <TextBlock Text="{Binding Publisher}" Background="DarkKhaki"/>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>
{
  "name": "Weather",
  "fullName": "Syrus.Plugins.Weather",
  "author": "Vladimír Antoš",
  "version": "0.0.0.1",
  "searchConfigurations": [
    {
      "language": "en",
      "command": "weather",
      "keyword": [ "weather", "forecast" ],
      "searchingPatterns": [
        {
          "name": "1",
          "pattern": "whats weather on {day}" //weather on {day}
        },
        {
          "name": 2,
          "pattern": "whats weather in {city}" //weather in {city}
        }
      ]
    },

    {
      "language": "cs",
      "command": "počasí",
      "keyword": [ "počasí", "předpověď" ],
      "searchingPatterns": [
        {
          "name": 1,
          "pattern": "jak bude v {day}"
        },
        {
          "name": 2,
          "pattern": "jak bude v {city}"
        }
      ]
    }
  ]
